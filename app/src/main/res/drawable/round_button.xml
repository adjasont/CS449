<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item
        android:bottom="20dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#C0C0C0" />
            </shape>
        </rotate>
    </item>
    <item
        android:height="48dp">
        <rotate
            android:fromDegrees="0"
            android:toDegrees="0"
            android:pivotX="100%"
            android:pivotY="100%">
            <shape android:shape="line">
                <stroke
                    android:color="#FFF"
                    android:width="2dp"/>
                <size
                    android:height="1dp"
                    android:width="1dp"/>
            </shape>
        </rotate>
    </item>
    <item
        android:right="21dp"
        android:bottom="0dp">
        <rotate
            android:fromDegrees="75"
            >
            <shape android:shape="line">
                <stroke
                    android:color="#FFF"
                    android:width="2dp"/>
            </shape>
        </rotate>
    </item>
    <item
        android:left="18dp"
        android:bottom="1dp"
        >
        <rotate
            android:fromDegrees="107">
            <shape android:shape="line">
                <stroke
                    android:color="#FFF"
                    android:width="2dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>